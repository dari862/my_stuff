#!/bin/sh
## Openbox Pipemenu to change openbox styles
. "$__distro_path_lib"
. "${__distro_path_root}/lib/common/WM"
. "${__distro_path_root}/lib/common/pipemenu"
. "${Distro_config_file}"

[ ! -f "${style_pipemenu_file}_${_panel_name_}" ] && "${__distro_path_root}"/bin/X11/pipemenu/create_pipemenu/style.sh

menuStart
cat "${style_pipemenu_file}_${_panel_name_}"
menuEnd
