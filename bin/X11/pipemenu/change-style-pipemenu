#!/bin/sh
## Openbox Pipemenu to change openbox styles
. "/usr/share/my_stuff/lib/common/Distro_path"
. "${__distro_path_root}/lib/common/WM"
. "${__distro_path_root}/lib/common/pipemenu"

[ ! -f "${style_pipemenu_file}" ] && ${__distro_path_root}/bin/X11/pipemenu/create_pipemenu/style.sh

menuStart
cat "${style_pipemenu_file}"
menuEnd
