#!/bin/sh
app_name="quickpassthrough"
say "Installing ${app_name}..." 1
repobin --sudo install HikariKnight/quickpassthrough || failed_to_run "failed to run ${app_name} ."
say 
say "quickpassthrough add for enabling GPU passthrough at OS level not at virt-manager level."
say "quickpassthrough add for enabling GPU passthrough at OS level not at virt-manager level."
say "quickpassthrough add for enabling GPU passthrough at OS level not at virt-manager level."
rm -f ${__distro_path_root}/system_files/bin/QuickPassthrough_installer
