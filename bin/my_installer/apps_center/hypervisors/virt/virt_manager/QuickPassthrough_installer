#!/bin/sh
. "$__distro_path_lib"
. "${__distro_path_root}/lib/common/common"
. "${__distro_path_root}/lib/common/my_installer_and_DB_dir"

say "Installing quickpassthrough..." 1
repobin --root install HikariKnight/quickpassthrough || failed_to_run "failed to run ${app_name} ."
say 
say "quickpassthrough add for enabling GPU passthrough at OS level not at virt-manager level."
remove_never_remove_bin "QuickPassthrough_installer" || :
