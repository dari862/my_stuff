#!/bin/sh
. "/usr/share/my_stuff/lib/common/Distro_path"
conky_path="${__distro_path_root}/bin/X11/yad/conky"
tint2_path="${__distro_path_root}/bin/X11/yad/tint2"

tint2manager(){
	${tint2_path}/tint2-manager
}

tint2edit(){
	${tint2_path}/tint2-edit
}

conkyedit(){
	${conky_path}/conky-edit
}

conkymanager(){
	${conky_path}/conky-manager
}

conkymove(){
	${conky_path}/conkymove
}

case "${1-}" in
		--tint2manager) tint2manager ;;
		--tint2edit) tint2edit ;;
		--conkyedit) conkyedit ;;
		--conkymanager) conkymanager ;;
		--conkymove) conkymove ;;
esac
