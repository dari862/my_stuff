#!/bin/sh
# Start panel
. "$__distro_path_lib"
		
"${__distro_path_root}"/bin/WM/panel_launcher &

# update notifcation
"${__distro_path_root}"/bin/WM/update-notification -n &

cd $HOME/.config/autostartscripts/
autostartscripts_list="$(ls 2>/dev/null)"
for script in ${autostartscripts_list};do
	./$script
done
