#!/bin/sh
. "$__distro_path_lib"
. "${__distro_path_root}/lib/common/WM"
. "${__distro_path_root}/lib/common/openbox"
. "${__distro_path_root}/lib/common/DB"
. "${Distro_config_file}"

_panel_name_="${1-}"

style_changer "$_panel_name_" "default"

if grep -q " never remove this line menu style are :jgmenu " "${BACKUP_OB_RC_FILE}";then
	change-ob-menu menu-icons.xml
fi
