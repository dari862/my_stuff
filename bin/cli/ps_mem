#!/bin/sh
# need superuser : use my-superuser
#===================================================================================
. "$__distro_path_lib"
failed_(){
	massage="${1-}"
	"[ERROR] $0 : ${massage}"
	exit 1
}

cd "${__distro_path_root}/bin/not_add_2_path/ps_mem/" || failed_ "Failed to cd ${ps_mem_path}."
my-superuser ./sudo_ps_mem
