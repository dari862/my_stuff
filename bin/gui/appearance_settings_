#!/bin/sh
#===================================================================================
. "$__distro_path_lib"
if command -v lxappearance >/dev/null 2>&1;then
	if [ "$HOME" = "/root" ];then
		lxappearance
	else
		lxappearance && ${__distro_path_root}/bin/X11/WM/reload_gtk23
	fi
else
	RiceSelector
fi	
