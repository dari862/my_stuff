origin = bottom-center
offset = 0x60

frame_color = "#3b4252"
separator_color = "#404859"

format = "<span size='x-large' font_desc='Cantarell,mplus Nerd Font 9' weight='bold' foreground='#f9f9f9'>%s</span>\n%b"

icon_position = top
icon_theme = "Papirus-Dark-Custom"

# transparency level.
transparency = 3

# Defines width in pixels of frame around the notification window.
# Set to 0 to disable.
frame_width = 6

font = "mplus Nerd Font 10"

[urgency_low]
timeout = 3
background = "#3b4252"
foreground = "#f9f9f9"
highlight = "#f9f9f9"

[urgency_normal]
timeout = 6
background = "#3b4252"
foreground = "#f9f9f9"
highlight = "#f9f9f9"

[urgency_critical]
timeout = 0
background = "#3b4252"
foreground = "#f9f9f9"
highlight = "#f9f9f9"
